<% include partial/header %>
<div class="container">
    <h1 style="text-align: center">Create A New Campground</h1>
    <div style="width: 30%; margin:25px auto;">
        <form action="/campgrounds" method="POST">
        <div class="form-group">
            <input class="form-control" type="text" placeholder="name" name="campgroundName">
        </div>
        <div class="form-group">
            <input class="form-control" type="text" placeholder="image url" name="campgroundImage">
        </div>   
            <button class="btn btn-lg btn-primary btn-block">Submit</button>
        </form>
        <a href="/campgrounds">Go Back</a>
    </div>

</div>
<% include partial/footer %>